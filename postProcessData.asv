fileName = 'DOE_Runs';

addpath(fileName);

files = dir(fileName);

for k = 3:length(files)
    close all
    load(files(k).name);
    rad = sArr(3,:,:);
    hoop = sArr(1,:,:);
    
    hold on
    
    for i = 1:3
        rMax((k-2),i) = max(abs(rad(:,:,i)));
        hMax((k-2),i) = max(abs(hoop(:,:,i)));
        figure(1)
        plot(rad(i,:,i),'LineWidth',1.5);
        figure(2)
        plot(hoop(i,:,i),'LineWidth',1.5);
    end
    
end
